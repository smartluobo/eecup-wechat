.container {
    height: 100vh;
    display: flex;
    background: #fbfbfb;
}

.top-space {
    height: 10rpx;
    background: #fbfbfb;
}

.btn {
    font-size: 14pt;
    height: 82rpx;
    line-height: 82rpx;
    border: 0;
    color: #fff;
}

.btn.enable {
    background: #f2cba4;
}

.btn.disable {
    background: #b9cfe4;
}

.avatarWrap {
    overflow: hidden;
    background-size: cover;
}

.showBefore:before,.showAfter:after {
    content: '\20';
    position: absolute;
    background-repeat: no-repeat;
}

.arrowsLeft:after {
    width: 28rpx;
    height: 28rpx;
    top: 50%;
    margin-top: -14rpx;
    right: 24rpx;
    background-size: 28rpx;
}

.btnWrap button {
    height: 84rpx;
    line-height: 84rpx;
    background: #88aed5;
    border-color: #88aed5;
    font-size: 32rpx;
    /* color: #fff; */
}

.emptyBg {
    width: 250rpx;
    height: 172rpx;
    margin: 228rpx auto 40rpx;
}

.emptyTips {
    text-align: center;
    font-size: 26rpx;
    color: #999;
    width: 100%;
    display: block;
    margin-bottom: 96rpx;
}

.emptyBtn {
    width: 210rpx;
    height: 66rpx;
    line-height: 66rpx;
    font-size: 28rpx;
    color: #f2cba4;
    border: 1rpx solid #f2cba4;
    border-radius: 12rpx;
    background: #fff;
}

.fix-iphonex-bottom {
    bottom: 68rpx!important;
}

.fix-iphonex-bottom::after {
    content: ' ';
    position: fixed;
    bottom: 0!important;
    height: 68rpx!important;
    width: 100%;
    background: #fff;
}

.more {
    width: 100%;
    height: 68rpx;
    text-align: center;
    line-height: 68rpx;
    background-color: #f5f5f5;
    border-top: 1rpx solid #e6e6e6;
    display: block;
}

.more_text {
    display: inline-block;
    vertical-align: top;
    font-size: 22rpx;
    color: #999;
}

.more i {
    display: inline-block;
    vertical-align: top;
    height: 68rpx;
    margin-right: 16rpx;
}

.more_over {
    /* background: url(http://image.domolife.cn/wechatIcon/low_2x.png) no-repeat center; */
    background-size: 36rpx 27rpx;
    width: 36rpx;
}

.more_load {
    /* background: url("http://image.domolife.cn/wechatIcon/load_flower.gif") no-repeat center; */
    background-size: 36rpx 36rpx;
    width: 36rpx;
}
button{
    border: 0rpx;
}
button::after{
    border: 0rpx;
}

.coupon {
    padding: 20rpx 14rpx;
    color: #999;
    box-sizing: border-box;
    background: #f6f6f6;
    min-height: 100vh;
}

.coupon .couponAvailable {
    color: #666;
    font-size: 26rpx;
    background: #ebf2f8;
    height: 72rpx;
    line-height: 72rpx;
    text-align: center;
    margin-bottom: 20rpx;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 9;
}

.coupon .couponAvailableBlock {
    height: 72rpx;
}

.coupon .saveWrapBlock {
    height: 100rpx;
}

.coupon .couponAvailable text {
    color: #f2cba4;
}

.coupon .wordEllipsis {
    width: 100%;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.coupon .itemWrap {
    border: 0.5PX solid rgba(213,221,230,0.73);
    margin-bottom: 20rpx;
    padding: 16rpx;
    background: #fff;
}

.coupon .itemWrap:last-child {
    margin: 0;
}

.coupon .itemBorder {
    border: 1PX solid rgba(124,167,210,0.7);
    padding: 6rpx;
}

.coupon .itemBorder1 {
    border: 1PX solid rgba(122,165,209,0.2);
}

.coupon .content {
    display: flex;
    height: 208rpx;
    align-items: center;
}

.coupon .contentLeft {
    width: 176rpx;
    height: 142rpx;
    border-right: 1px dashed rgba(124,167,210,0.2);
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
}

.coupon .leftWrap {
    width: 100%;
    text-align: center;
}

.coupon .leftNum {
    font-size: 72rpx;
    color: #f2cba4;
}

.coupon .money,.coupon .discount {
    font-size: 36rpx;
}

.coupon .discount text {
    font-size: 40rpx;
}

.coupon .leftText {
    display: block;
    font-size: 35rpx;
    color: #f2cba4;
}

.coupon .contentRight {
    position: relative;
    display: flex;
    overflow: hidden;
    height: 100%;
    flex: 1;
    padding-left: 56rpx;
    align-items: center;
    background: url(https://www.eecup.cn/tea/image/eecup/coupons_background.png) no-repeat right;
    background-size: 45%;
    margin-right: 14rpx;
}

.coupon .rightText {
    width: 336rpx;
    flex-direction: column;
}

.coupon .title {
    font-size: 32rpx;
    color: #333;
    padding-bottom: 20rpx;
}

.coupon .time {
    font-size: 24rpx;
}

.coupon .radioWrap {
    display: flex;
    flex: 1;
    justify-content: flex-end;
}

.coupon .radio {
    width: 44rpx;
    height: 44rpx;
    border-radius: 50%;
    border: 1px solid #d0d0d0;
}

.coupon .checked {
    background: url(https://www.eecup.cn/tea/image/eecup/icon/coupon/coupon_checked.png) no-repeat center;
    background-size: cover;
    border: none;
}

.coupon .ruleUse {
    position: absolute;
    font-size: 22rpx;
    bottom: 14rpx;
    right: 26rpx;
}

.coupon .rotate {
    transform: rotate(180deg);
}

.coupon .info {
    border-top: 1px dashed rgba(124,167,210,0.2);
    font-size: 24rpx;
    color: #666;
    margin: 30rpx 30rpx 0;
    padding: 30rpx 0 24rpx;
}

.coupon .info .infoText {
    padding-top: 8rpx;
    word-break: break-all;
    word-wrap: break-word;
}

.coupon .ruleText {
    padding-right: 10rpx;
}

.coupon .arrow {
    width: 12rpx;
    height: 12rpx;
    display: inline-block;
    vertical-align: 2rpx;
    background: url(https://www.eecup.cn/tea/image/eecup/icon/open_icon.png) no-repeat center;
    background-size: contain;
}

.coupon .order {
    background: url(https://www.eecup.cn/tea/image/eecup/icon/coupon_empty.png) no-repeat center;
    background-size: contain;
    margin-bottom: 0;
    width: 250rpx;
    height: 172rpx;
}

.coupon .emptyTips {
    margin-bottom: 60rpx;
    color: #999;
    font-size: 26rpx;
}

.coupon .emptyBg {
    margin: 234rpx auto 36rpx;
}

.coupon .saveWrap {
    width: 100%;
    height: 120rpx;
    background: #fff;
    box-sizing: border-box;
    padding: 0 30rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    position: fixed;
    bottom: 0;
    left: 0;
}

.coupon .save {
    width: 100%;
    height: 84rpx;
    text-align: center;
    background: rgba(124,167,210,0.9);
    border-radius: 6rpx;
    color: #fff;
    line-height: 84rpx;
    font-size: 32rpx;
}

.itemWrapB {
    background: #fff;
    border-radius: 12rpx;
    overflow: hidden;
    padding: 40rpx 40rpx 10rpx;
    margin-bottom: 20rpx;
    position: relative;
}

.itemWrapB:nth-last-child(1) {
    margin-bottom: 0;
}

.itemWrapB::before,.itemWrapB::after {
    content: '';
    width: 18rpx;
    height: 18rpx;
    background: #f6f6f6;
    border-radius: 50%;
    position: absolute;
    top: 165rpx;
}

.itemWrapB::before {
    left: -9rpx;
}

.itemWrapB::after {
    right: -9rpx;
}

.itemWrapB .itemContent-top {
    display: flex;
    border-bottom: 1rpx dashed #e6e6e6;
}

.itemWrapB .top-left {
    flex: 1;
}

.itemWrapB .top-right {
    width: 184rpx;
    height: 132rpx;
    background-image: url(https://www.eecup.cn/tea/image/eecup/icon/coupon/coupon_cup.png);
    background-position: right bottom;
    background-repeat: no-repeat;
    background-size: 184rpx 88rpx;
    text-align: right;
}

.itemWrapB .cardName {
    font-size: 30rpx;
    line-height: 42rpx;
    color: #333;
    margin-bottom: 20rpx;
}

.itemWrapB .limitTime {
    font-size: 24rpx;
    line-height: 33rpx;
    color: #999;
}

.itemWrapB .discount {
    color: #f2cba4;
    font-size: 72rpx;
    line-height: 72rpx;
}

.itemWrapB .discount text {
    font-size: 24rpx;
    line-height: 33rpx;
}

.itemWrapB .btnBar {
    display: flex;
    margin-top: 8rpx;
}

.itemWrapB .btnBar>view {
    flex: 1;
}

.itemWrapB .ruleBtn {
    display: flex;
    align-items: center;
    font-size: 24rpx;
    line-height: 33rpx;
    color: #999;
}

.itemWrapB .arrow {
    margin-left: 14rpx;
}

.itemWrapB .nowUse {
    display: flex;
    justify-content: flex-end;
}

.itemWrapB .nowUse .nowUse-btn {
    background: #f2cba4;
    color: #fff;
    font-size: 24rpx;
    line-height: 56rpx;
    text-align: center;
    width: 156rpx;
    height: 56rpx;
    border-radius: 28rpx;
}

.itemWrapB .ruleInfo {
    margin-top: 30rpx;
    margin-bottom: 22rpx;
}

.itemWrapB .ruleInfoText {
    font-size: 24rpx;
    line-height: 33rpx;
    color: #999;
}

.botoomBanner {
    width: 100%;
    height: 144rpx;
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
}

.bannerImg {
    width: 100%;
    height: 100%;
}